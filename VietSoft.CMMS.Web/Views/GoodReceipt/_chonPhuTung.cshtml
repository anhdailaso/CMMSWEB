@using VietSoft.CMMS.Web.Resources
@model IEnumerable<VietSoft.CMMS.Web.Models.PhuTungModel>

    @if (Model.Count() > 0)
    {
    <table class="table table-normal-padding tblchonphutung">
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="tr-supplies">
                        <td class="w-30">@item.MS_PT</td>
                        <td class="w-50">@item.TEN_PT</td>
                        <td class="w-20">
                            <div class="">
                                <input class="form-check-input" type="checkbox">
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <span>@ViewText.LBL_KHONGCO_DULIEU</span>
    }
