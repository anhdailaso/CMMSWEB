@using VietSoft.CMMS.Web.Resources
@model IEnumerable<VietSoft.CMMS.Web.Models.ChonDanhSachPhuTungXuatModel>

    @if (Model.Count() > 0)
    {
        <table class="table table-normal-padding tblAddSuppliesSeleced">
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="tr-supplies">
                        <td class="w-20">@item.MS_PT</td>
                        <td class="w-40">@item.MS_DH_NHAP_PT</td>
                    <td class="w-15">@item.SL_XUAT</td>
                    <td class="w-15">@item.SL_TON</td>
                        <td class="w-10">
                            <div class="">
                                <input class="form-check-input" type="checkbox">
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <span>@ViewText.TXT_KHONGCO_DULIEU</span>
    }
